# 프로덕션 환경 설정
VITE_API_BASE_URL=https://api.moneybuddy.com
VITE_API_TIMEOUT=15000
VITE_API_RETRY_COUNT=2
VITE_API_RETRY_DELAY=2000

# MSW 설정 (프로덕션에서는 비활성화)
VITE_USE_MSW=false
VITE_MSW_DEBUG=false
VITE_MSW_LOG_REQUESTS=false
VITE_MSW_LOG_RESPONSES=false

# 소셜 로그인 설정 (실제 클라이언트 ID)
VITE_KAKAO_CLIENT_ID=production_kakao_client_id
VITE_GOOGLE_CLIENT_ID=production_google_client_id
VITE_NAVER_CLIENT_ID=production_naver_client_id

# 기타 설정
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=production
